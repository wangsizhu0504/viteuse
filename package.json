{
  "name": "vite-admin-start",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.6.3",
  "scripts": {
    "start": "npm run dev",
    "test": "vitest",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "dev:host": "vite --host",
    "dev:open": "vite --open",
    "preview": "vite preview",
    "coverage": "vitest --coverage",
    "typecheck": "npx vue-tsc --noEmit",
    "preview:host": "vite preview --host",
    "preview:open": "vite preview --open",
    "lint": "eslint --ext .ts,.js,.jsx,.vue .",
    "lint:fix": "eslint --fix --ext .ts,.js,.jsx,.vue .",
    "build:debug": "cross-env NODE_ENV=debug vite build"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@vueuse/core": "^10.0.0",
    "ant-design-vue": "^3.2.20",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.34",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.16",
    "@kriszu/eslint-config": "^1.0.14",
    "@types/nprogress": "^0.2.0",
    "@unocss/eslint-config": "^0.53.0",
    "@vitejs/plugin-legacy": "^4.0.5",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "echarts": "^5.4.2",
    "eslint": "^8.38.0",
    "lint-staged": "^13.2.1",
    "plop": "^3.1.2",
    "pnpm": "^8.2.0",
    "typescript": "^5.0.0",
    "unocss": "^0.52.0",
    "unplugin-auto-import": "^0.16.0",
    "unplugin-vue-components": "^0.25.0",
    "vite": "^4.2.1",
    "vite-bundle-visualizer": "^0.7.0",
    "vite-plugin-inspect": "^0.7.20",
    "vite-plugin-vue-devtools": "^0.2.1",
    "vite-ssg": "^0.22.2",
    "vite-ssg-sitemap": "^0.5.0",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.2.0"
  },
  "resolutions": {
    "vue-demi": "npm:vue-demi@v0.14.5",
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --cache --fix"
  }
}
